                    android:layout_alignBottom="@+id/myReqDettv10"
                    android:layout_alignLeft="@+id/myReqDettv19"
                    android:textColor="@color/textbody" 
                    />
            </LinearLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="1dip"
                android:layout_below="@+id/myReqDettv20"
                android:background="#C0C0C0" />

            <!-- 11 -->

