<?xml version="1.0" encoding="utf-8"?>

<!--
	This Xml File is used to Display the full Detail of the My Requirement 
	this file is Open Onclick Of first Text View Of My requirement 
	Java File:- MyRequirementDetailActivity.java
-->
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/myReqDetLayout"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/bg" >

    <TextView
        android:id="@+id/myReqDetSubHeaderTv"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:gravity="center"
        android:textColor="@color/textbody"
        android:textStyle="bold" />

    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_below="@+id/myReqDetSubHeaderTv"
        android:layout_marginBottom="30dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/listview_item_customshape"
