                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/myReqDettv21"
                    android:layout_alignBottom="@+id/myReqDettv21"
                    android:layout_alignLeft="@+id/myReqDettv20"
                    android:textColor="@color/textbody" />
