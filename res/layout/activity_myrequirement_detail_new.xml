                    android:id="@+id/myReqDettv11"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/myReqDettv1"
                    android:layout_alignBottom="@+id/myReqDettv1"
                    android:layout_toRightOf="@+id/myReqDettv1"
                    android:textColor="@color/textbody" />
            </LinearLayout>

