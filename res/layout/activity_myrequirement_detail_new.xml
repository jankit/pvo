                    android:id="@+id/myReqDettv2"
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/myReqDettv1"
                    android:layout_margin="5dp"
                    android:drawableLeft="@drawable/triangle"
                    android:drawablePadding="10dp"
                    android:text="Preferred Locations"
                    android:textColor="@android:color/black"
                    android:textSize="13sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/myReqDettv12"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/myReqDettv2"
                    android:layout_alignBottom="@+id/myReqDettv2"
                    android:layout_alignLeft="@+id/myReqDettv11"
                    android:textColor="@color/textbody" />
