                android:layout_below="@+id/myReqDettv18"
                android:background="#C0C0C0" />

            <!-- 9 -->

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" 
                android:gravity="center">

                <!-- Building Type -->

                <TextView
                    android:id="@+id/myReqDettv9"
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/myReqDettv8"
                    android:layout_margin="5dp"
                    android:drawableLeft="@drawable/triangle"
                    android:drawablePadding="10dp"
                    android:text="Building Type"
                    android:textColor="@android:color/black"
                    android:textSize="13sp"
                    android:textStyle="bold"
                     />

                <TextView
                    android:id="@+id/myReqDettv19"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/myReqDettv9"
                    android:layout_alignBottom="@+id/myReqDettv9"
                    android:layout_alignLeft="@+id/myReqDettv18"
                    android:textColor="@color/textbody" 
                    />
            </LinearLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="1dip"
                android:layout_below="@+id/myReqDettv19"
                android:background="#C0C0C0" 
                android:gravity="center"/>
            <!-- 10 -->

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <!-- Area -->

                <TextView
                    android:id="@+id/myReqDettv10"
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/myReqDettv9"
                    android:layout_margin="5dp"
                    android:drawableLeft="@drawable/triangle"
                    android:drawablePadding="10dp"
